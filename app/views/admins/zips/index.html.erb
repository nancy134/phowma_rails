<p id="notice"><%= notice %></p>

<h1>Admins Zips</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins_zips.each do |admins_zip| %>
      <tr>
        <td><%= admins_zip.code %></td>
        <td><%= link_to 'Show', admins_zip %></td>
        <td><%= link_to 'Edit', edit_admins_zip_path(admins_zip) %></td>
        <td><%= link_to 'Destroy', admins_zip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admins Zip', new_admins_zip_path %>
