<table class="table">
  <thead>
    <tr>
      <th>Date</th>
      <th>State</th>
      <th>Position</th>
      <th>District</th>
      <th>Last Name</th>
      <th>First Name</th>
      <th colspan="3"></th>
    </tr>
  <thead>
  <tbody>
    <%= render 'filter', search:@search %>
    <% @admins_campaigns.each do |admins_campaign| %>
      <tr>
        <td><%= admins_campaign.election.date %></td>
        <td><%= admins_campaign.election.office.state.name %></td>
        <td><%= admins_campaign.election.office.position %></td>
        <% if admins_campaign.election.office.district %>
          <td><%= admins_campaign.election.office.district.name %></td>
        <% else %>
          <td></td>
        <% end %>
        <td><%= admins_campaign.politician.last_name %></td>
        <td><%= admins_campaign.politician.first_name %></td>
        <td><%= link_to 'Show', admins_campaign %></td>
        <td><%= link_to 'Edit', edit_admins_campaign_path(admins_campaign) %></td>
        <td><%= link_to 'Destroy', admins_campaign, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @admins_campaigns %>
