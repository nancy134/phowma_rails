<table class="table">
  <thead>
    <tr>
      <th>Id</td>
      <th>Name</th>
      <th>State</th>
      <th>Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <%= render 'filter', search:@search %>
    <% @admins_districts.each do |admins_district| %>
      <tr>
        <td><%= admins_district.id %></td>
        <td><%= admins_district.name %></td>
        <td><%= admins_district.state.name %></td>
        <td><%= admins_district.number %></td>
        <td><%= link_to 'Show', admins_district %></td>
        <td><%= link_to 'Edit', edit_admins_district_path(admins_district) %></td>
        <td><%= link_to 'Destroy', admins_district, method: :delete, data: {confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @admins_districts %>
