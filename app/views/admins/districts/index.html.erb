<p id="notice"><%= notice %></p>

<h1>Admins Districts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins_districts.each do |admins_district| %>
      <tr>
        <td><%= admins_district.name %></td>
        <td><%= admins_district.state_id %></td>
        <td><%= link_to 'Show', admins_district %></td>
        <td><%= link_to 'Edit', edit_admins_district_path(admins_district) %></td>
        <td><%= link_to 'Destroy', admins_district, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= form_tag import_admins_districts_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>

<br>

<%= link_to 'New Admins District', new_admins_district_path %>
