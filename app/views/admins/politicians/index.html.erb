<p id="notice"><%= notice %></p>

<p>Admins Politicians</p>
<%= will_paginate @admins_politicians %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Party</th>
      <th>Position</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins_politicians.each do |admins_politician| %>
      <tr>
        <td><%= admins_politician.name %></td>
        <td><%= admins_politician.first_name %></td>
        <td><%= admins_politician.last_name %></td>
        <td><%= admins_politician.party %></td>
        <td><%= admins_politician.position %></td>
        <td><%= admins_politician.state.name %></td>
        <td><%= link_to 'Show', admins_politician %></td>
        <td><%= link_to 'Edit', edit_admins_politician_path(admins_politician) %></td>
        <td><%= link_to 'Destroy', admins_politician, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @admins_politicians %>
<br>

<%= form_tag import_admins_politicians_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>
<br>

<%= link_to 'New Admins Politician', new_admins_politician_path %>
