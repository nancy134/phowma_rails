<div class="page-header">
  <h2>Politicians</h2>
</div>

<div class="row">
  <div class="col-md-6">
    <%= link_to 'New Politician', new_admins_politician_path, {:class => "btn btn-primary"} %>
  </div>
  <div class="col-md-6">
    <span class="pull-right"><button type="button" class="btn btn-link" data-toggle="collapse" data-target="#list_search">Search and Filter</button></span>
  </div>
</div>

<%= render 'list', admins_politician: @admins_politician %>

<br>
<%= form_tag import_admins_politicians_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>



