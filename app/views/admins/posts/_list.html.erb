<table class="table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Type</th>
      <th>Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <%= render 'filter', search:@search %>
    <% @admins_posts.each do |admins_post| %>
      <tr>
        <td><%= admins_post.social_date %></td>
        <td><%= admins_post.social_type %></td>
        <td><%= admins_post.social_id %></td>
        <td><%= link_to 'Show', admins_post %></td>
        <td><%= link_to 'Edit', admins_post %></td>
        <td><%= link_to 'Destroy', admins_post, method: :delete, data: {confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate @admins_posts %>

